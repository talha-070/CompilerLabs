PROCEDIMIENTO main() {
    ENTERO i -> 0;
    ENTERO j;
    ENTERO rows -> 5;
    ENTERO count -> 0;

    POR (i) EN (1,(2*rows),(i+2)) {
        POR (j) EN (1,i,1) {
            count -> 0;
        }
        count -> 0;

        SI (i > rows) ENTONCES {
            count -> -10;
            POR (j) EN (1,i,1) {
                count -> 0;
            }
        }
    }

    REGRESA 0;
}